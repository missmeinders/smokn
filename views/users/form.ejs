    <form action="/users" method="post" role="form">
      <input type="hidden">

      <div class="row">
        <div class="three columns">
          <label for="email">Email</label>
            <input type="text" id="email">
          </div>
      </div>
      <div class="row">
        <div class="three colum ns">
          <label for="password">Password</label>
            <input type="password" id="password">
          </div>
        <div class="three columns">
          <label for="passwordConfirmation">Password Confirmation</label>
            <input type="password" id="passwordConfirmation">
          </div>
      </div>
      <div class="row">
        <div class="nine columns">
          <label for="userDescription">Tell us about yourself in 140 characters or fewer.</label>
          <textarea class="u-full-width" type="text" id="userDescription"></textarea>
        </div>
      </div>

      <div class="row">
        <div class="nine columns">
          <label for="birthday">Birthday</label>
          <input type="text" id="birthday" />
        </div>
      </div>

      <div class="row">
        <div class="nine columns">
          <label for="userGender">I identify as a: </label>
          <select class="u-full-width" id="userGender">
            <option value="woman">Woman</option>
            <option value="man">Man</option>
            <option value="nonBinary">Non-Binary Gender</option>
          </select>
        </div>
      </div>

      <div class="row">
        <div class="nine columns">
        <label for="matchPreference">I'm looking for: </label>
          <input type="checkbox" value="woman"><span class="label-body">Women</label>
          <input type="checkbox" value="man"><span class="label-body">Men</label>
          <input type="checkbox" value="nonBinary"><span class="label-body">Non-Binary Gendered</label>
        </div>
      </div>

      <div class="row">
        <div class="nine columns">
        <label for="type">I smoke: </label>
          <input type="checkbox" value="cigarettes"><span class="label-body">Cigarettes</label>
          <input type="checkbox" value="marijuana"><span class="label-body">Marijuana</label>
          <input type="checkbox" value="vaporizer"><span class="label-body">Vaporizer</label>
          <input type="checkbox" value="cloves"><span class="label-body">Cloves</label>
          <input type="checkbox" value="pipe"><span class="label-body">Pipes</label>
        </div>
      </div>

      <div class="row">
        <div class="three columns">
          <label for="smokespots">Where's your spot?</label>
            <input type="text" id="smokespots">
          </div>
        </div>

      <div class="row">
        <div class="three columns">
          <label for="photo_url">Profile Picture URL</label>
          <input type="text" id="photo_url" >
        </div>
      </div>
      <input class="button-primary" type="submit" value="Submit">
</form>
